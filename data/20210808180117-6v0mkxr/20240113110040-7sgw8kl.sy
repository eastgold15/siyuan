{"ID":"20240113110040-7sgw8kl","Spec":"1","Type":"NodeDocument","Properties":{"icon":"1f4f1","id":"20240113110040-7sgw8kl","title":"Mobile App","type":"doc","updated":"20250510111100"},"Children":[{"ID":"20250510111100-ixtc78u","Type":"NodeThematicBreak","Properties":{"id":"20250510111100-ixtc78u","updated":"20250510111100"}},{"ID":"20250510111100-mco5kyh","Type":"NodeParagraph","Properties":{"id":"20250510111100-mco5kyh","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"title: Mobile App\ndate: 2024-01-13T11:00:40Z\nlastmod: 2025-05-10T10:48:17Z"}]},{"ID":"20250510111100-m31g9pc","Type":"NodeThematicBreak","Properties":{"id":"20250510111100-m31g9pc","updated":"20250510111100"}},{"ID":"20250510111100-gb0wuth","Type":"NodeThematicBreak","Properties":{"id":"20250510111100-gb0wuth","updated":"20250510111100"}},{"ID":"20250510111100-76rkn6r","Type":"NodeParagraph","Properties":{"id":"20250510111100-76rkn6r","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"title: Mobile App"},{"Type":"NodeBr","Data":"br"},{"Type":"NodeText","Data":"date: 2024-01-13T11:00:40Z"},{"Type":"NodeBr","Data":"br"},{"Type":"NodeText","Data":"lastmod: 2025-01-17T17:19:14Z"}]},{"ID":"20250510111100-mjhrvu1","Type":"NodeThematicBreak","Properties":{"id":"20250510111100-mjhrvu1","updated":"20250510111100"}},{"ID":"20250510111100-zvy573z","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20250510111100-zvy573z","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Overview"}]},{"ID":"20250510111100-g2voc70","Type":"NodeParagraph","Properties":{"id":"20250510111100-g2voc70","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"SiYuan provides Android, iOS and HarmonyOS apps, and has been listed on some mobile app stores. Please search for "},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"思源笔记"},{"Type":"NodeText","Data":"​ or "},{"Type":"NodeTextMark","TextMarkType":"code","TextMarkTextContent":"SiYuan"},{"Type":"NodeText","Data":"​ in the app store."}]},{"ID":"20250510111100-zips4mm","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20250510111100-zips4mm","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Installation and updates"}]},{"ID":"20250510111100-fkb3jh4","Type":"NodeParagraph","Properties":{"id":"20250510111100-fkb3jh4","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Currently available on the app store:"}]},{"ID":"20250510111100-nps92mx","Type":"NodeList","ListData":{},"Properties":{"id":"20250510111100-nps92mx","updated":"20250510111100"},"Children":[{"ID":"20250510111100-il4z6bd","Type":"NodeListItem","ListData":{"BulletChar":42,"Marker":"Kg=="},"Properties":{"id":"20250510111100-il4z6bd","updated":"20250510111100"},"Children":[{"ID":"20250510111100-jvei95l","Type":"NodeParagraph","Properties":{"id":"20250510111100-jvei95l","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Mi"}]}]},{"ID":"20250510111100-wpao9yx","Type":"NodeListItem","ListData":{"BulletChar":42,"Marker":"Kg=="},"Properties":{"id":"20250510111100-wpao9yx","updated":"20250510111100"},"Children":[{"ID":"20250510111100-lhmd3bc","Type":"NodeParagraph","Properties":{"id":"20250510111100-lhmd3bc","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Huawei"}]}]},{"ID":"20250510111100-niy2m42","Type":"NodeListItem","ListData":{"BulletChar":42,"Marker":"Kg=="},"Properties":{"id":"20250510111100-niy2m42","updated":"20250510111100"},"Children":[{"ID":"20250510111100-psml84j","Type":"NodeParagraph","Properties":{"id":"20250510111100-psml84j","updated":"20250510111100"},"Children":[{"Type":"NodeText","Data":"Honor"}]}]},{"ID":"20250510111100-upkgp9o","Type":"NodeListItem","ListData":{"BulletChar":42,"Marker":"Kg=="},"Properties":{"id":"20250510111100-upkgp9o","updated":"20250510111100"},"Children":[{"ID":"20250510111100-2zh9110","Type":"NodeParagraph","Properties":{"id":"20250510111100-2zh9110","updated":"20250510111100"},"Children":[