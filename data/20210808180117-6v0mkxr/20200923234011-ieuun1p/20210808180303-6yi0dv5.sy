ta":"      "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"* You can also enable"},{"Type":"NodeText","Data":"  "},{"Type":"NodeText","Data":"  "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextContent":"Settings"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"-"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextContent":"Cloud"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"-"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextContent":"Generate conflict documentation when syncing conflicts"},{"Type":"NodeTextMark","TextMarkType":"sup kbd"},{"Type":"NodeText","Data":"​\n​"},{"Type":"NodeText","Data":"    "},{"Type":"NodeText","Data":"    "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"* Support browsing and downloading cloud snapshots in"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextContent":"Data History"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"-"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextContent":"Data snapshot"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeTextMark","TextMarkType":"sup kbd","TextMarkTextContent":"-"},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":" "},{"Type":"NodeText","Data":"​"},{"Type":"NodeTextMark","TextMarkType":"kbd","TextMarkTextConten